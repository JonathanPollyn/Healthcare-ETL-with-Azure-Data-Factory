{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "pharmacy_source",
						"type": "DatasetReference"
					},
					"name": "Year"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "YearSold"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Date Sold} as string,",
				"          Med_name as string,",
				"          Med_class as string,",
				"          {Quantity Sold} as string,",
				"          Price as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Year",
				"Year derive({Date Sold} = right({Date Sold}, 4)) ~> YearSold"
			]
		}
	}
}